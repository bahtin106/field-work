{
  "expo": {
    "name": "Монитор",
    "slug": "work-orders-app",
    "icon": "./icon.png",
    "version": "1.0.1",
    "orientation": "portrait",
    "scheme": "workorders",
    "platforms": [
      "android"
    ],
    "jsEngine": "hermes",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "splash": {
      "image": "./assets/splash/splashscreen_logo.png",
      "backgroundColor": "#f2f6fa",
      "resizeMode": "contain"
    },
    "android": {
      "softwareKeyboardLayoutMode": "resize",
      "package": "com.bahtin106.workordersapp",
      "backgroundColor": "#f2f6fa"
    },
    "extra": {
      "eas": {
        "projectId": "3c645715-1637-4502-979c-d4690c6cf1e2"
      }
    },
    "plugins": [
      "expo-router",
      "expo-splash-screen"
    ],
    "androidNavigationBar": {
      "backgroundColor": "#FFFFFF",
      "barStyle": "dark-content",
      "visible": "visible"
    },
    "androidStatusBar": {
      "backgroundColor": "#f2f6fa",
      "barStyle": "dark-content",
      "translucent": false
    }
  }
}